CREATE TABLE `customer` (
  `Name` varchar(300) NOT NULL,
  `CardNo` varchar(300) DEFAULT NULL,
  `Descriot` varchar(300) DEFAULT NULL,
  `CtfTp` varchar(300) DEFAULT NULL,
  `CtfId` varchar(300) DEFAULT NULL,
  `Gender` varchar(300) DEFAULT NULL,
  `Birthday` varchar(300) DEFAULT NULL,
  `Address` varchar(300) DEFAULT NULL,
  `Zip` varchar(300) DEFAULT NULL,
  `Dirty` varchar(300) DEFAULT NULL,
  `District1` varchar(300) DEFAULT NULL,
  `District2` varchar(300) DEFAULT NULL,
  `District3` varchar(300) DEFAULT NULL,
  `District4` varchar(300) DEFAULT NULL,
  `District5` varchar(300) DEFAULT NULL,
  `District6` varchar(300) DEFAULT NULL,
  `FirstNm` varchar(300) DEFAULT NULL,
  `LastNm` varchar(300) DEFAULT NULL,
  `Duty` varchar(300) DEFAULT NULL,
  `Mobile` varchar(300) DEFAULT NULL,
  `Tel` varchar(300) DEFAULT NULL,
  `Fax` varchar(300) DEFAULT NULL,
  `EMail` varchar(300) DEFAULT NULL,
  `Nation` varchar(300) DEFAULT NULL,
  `Taste` varchar(300) DEFAULT NULL,
  `Education` varchar(300) DEFAULT NULL,
  `Company` varchar(300) DEFAULT NULL,
  `CTel` varchar(300) DEFAULT NULL,
  `CAddress` varchar(300) DEFAULT NULL,
  `CZip` varchar(300) DEFAULT NULL,
  `Family` varchar(300) DEFAULT NULL,
  `Version` varchar(300) DEFAULT NULL,
  `id` varchar(300) NOT NULL
) ENGINE=MyISAM DEFAULT CHARSET=utf8;

